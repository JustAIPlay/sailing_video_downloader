{
  "manifest_version": 3,
  "name": "大航海视频下载神器",
  "version": "1.0",
  "description": "从飞书多维表格批量下载视频/附件，支持脚本号筛选、自动去重和断点续传。",
  "permissions": [
    "downloads",
    "storage",
    "sidePanel"
  ],
  "host_permissions": [
    "https://open.feishu.cn/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "打开大航海视频下载神器",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "side_panel": {
    "default_path": "popup.html"
  }
}
